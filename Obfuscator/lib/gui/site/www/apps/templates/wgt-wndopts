function openhelp()
{
    var url = '/support/doc/obfusui/help.shtml#';
    {
	var p = location.pathname;
	var fname = p.substr(p.lastIndexOf('/')+1);
	url += fname;
    }		    
    openwnd(url,'helpwindow',
	'directories=0,location=0,menubar=0,personalbar=0,' +
	'toolbar=0,resizable=1,status=0,scrollbars=1'); 
}
