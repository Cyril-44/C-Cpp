<script language=javascript>
setFooterBottom(0);

window['export-data'] = 1;
function show_build_output()
{
    var d = ge_by_id('outputmsg');
    d.innerHTML = window['export-data-log'];

    var ds = ge_by_id('resultstr');
    ds.innerHTML = window['export-data-syms'];
