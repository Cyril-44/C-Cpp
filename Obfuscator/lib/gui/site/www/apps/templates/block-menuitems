<table><tr><td>
<table   class=menuitems-as-content>
<%
    foreach my $i (@$mitems)
    {
	if ($i->[0] =~ '_SEPARATOR_') {
    	    $forge->include('item-menuitem-separator',$i,$prjid); 
	} else {
%>
<tr><% $forge->include('item-menuitem-as-content',$i,$prjid); %><%$ $tip{"menu." . $i->[0]} %><%
	}
    }
%>
</table>
</td></table>